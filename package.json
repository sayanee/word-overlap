{
  "name": "word-overlap",
  "version": "2.3.2",
  "description": "overlap of words in a sentence or phrase to determine whether they are referring to the same context",
  "keywords": [
    "overlap",
    "string",
    "words"
  ],
  "main": "index.js",
  "scripts": {
    "test": "grunt && mocha test && mocha test --require blanket -R html-cov > coverage.html && node example.js",
    "start": "node example.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sayanee/word-overlap.git"
  },
  "author": "Sayanee <sayanee@gmail.com> (www.sayan.ee)",
  "license": "MIT",
  "devDependencies": {
    "blanket": "1.1.6",
    "chai": "2.1.0",
    "colors": "1.0.3",
    "coveralls": "2.11.2",
    "grunt": "0.4.5",
    "grunt-bump": "0.3.0",
    "grunt-contrib-jshint": "0.11.0",
    "grunt-jscs": "1.5.0",
    "grunt-jsonlint": "1.0.4",
    "mocha": "2.1.0",
    "mocha-lcov-reporter": "0.0.1"
  },
  "config": {
    "blanket": {
      "pattern": [
        "index.js",
        "lib/lib.js"
      ],
      "data-cover-never": "node_modules"
    }
  },
  "dependencies": {
    "is-number": "1.1.0",
    "natural": "0.2.0",
    "pluralize": "1.1.2"
  }
}
